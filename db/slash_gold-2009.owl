        <rdfs:subPropertyOf rdf:resource="&gold;dataStructureRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/linguistics/gold/hasSpecification -->

    <owl:ObjectProperty rdf:about="&gold;hasSpecification">
