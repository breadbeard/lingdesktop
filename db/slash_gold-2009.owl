            >This relates either a FeatureStructure or a FeatureConstraint to its type, expressed by an instance of PartOfSpeech.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&gold;dataStructureRelation"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&gold;FeatureConstraint"/>
                    <rdf:Description rdf:about="&gold;FeatureStructure"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/linguistics/gold/hasValue -->

    <owl:ObjectProperty rdf:about="&gold;hasValue">
        <rdfs:label xml:lang="en"></rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI"
            >http://purl.org/linguistics/gold</rdfs:isDefinedBy>
        <rdfs:comment
            >This relates a SimpleSpecification to some instance of LinguisticPropertyValue.</rdfs:comment>
        <rdfs:comment
            >The relation between a feature instance and a feature-value instance.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&gold;dataStructureRelation"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&gold;LinguisticProperty"/>
                    <rdf:Description rdf:about="&gold;SimpleSpecification"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&gold;MorphosemanticProperty"/>
                    <rdf:Description rdf:about="&gold;MorphosyntacticProperty"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/linguistics/gold/head -->

    <owl:ObjectProperty rdf:about="&gold;head">
        <rdfs:label xml:lang="en"></rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI"
            >http://purl.org/linguistics/gold</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="&gold;Phrase"/>
        <rdfs:subPropertyOf rdf:resource="&gold;hasSyntacticConstituent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/linguistics/gold/hypernym -->

    <owl:ObjectProperty rdf:about="&gold;hypernym">
        <rdfs:label xml:lang="en"></rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI"
            >http://purl.org/linguistics/gold</rdfs:isDefinedBy>
        <rdfs:comment>hypernym</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&gold;lexicalRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/linguistics/gold/inLanguage -->

    <owl:ObjectProperty rdf:about="&gold;inLanguage">
        <rdfs:label xml:lang="en"></rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI"
            >http://purl.org/linguistics/gold</rdfs:isDefinedBy>
        <rdfs:comment
            >The relation that holds between a LinguisticSign and a Language. The sign is said to be a member or part of that language.</rdfs:comment>
        <rdfs:range rdf:resource="&gold;HumanLanguageVariety"/>
        <rdfs:domain rdf:resource="&gold;LinguisticUnit"/>
    </owl:ObjectProperty>
    


