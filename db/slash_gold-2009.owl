        <rdfs:domain rdf:resource="&gold;FeatureStructure"/>
        <rdfs:subPropertyOf rdf:resource="&gold;dataStructureRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/linguistics/gold/hasSyntacticConstituent -->

    <owl:ObjectProperty rdf:about="&gold;hasSyntacticConstituent">
