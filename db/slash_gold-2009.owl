    <VoicingProperty rdf:about="&gold;Voiceless">
        <rdf:type rdf:resource="&gold;GlottalStrictureProperty"/>
    </VoicingProperty>
    


    <!-- http://purl.org/linguistics/gold/VolitiveForce -->

    <ForceProperty rdf:about="&gold;VolitiveForce">
        <bibliography:hasCitation rdf:datatype="&xsd;anyURI"
            >http://purl.org/linguistics/gold-bibliography;Palmer2001_7082</bibliography:hasCitation>
        <rdfs:comment xml:lang="en"
            >VolitiveForce indicates that the speaker is willing to perform some action [Palmer 2001, 76].</rdfs:comment>
    </ForceProperty>
    


    <!-- http://purl.org/linguistics/gold/WeakObligativeModality -->

    <ModalityProperty rdf:about="&gold;WeakObligativeModality">
