        <rdfs:subPropertyOf rdf:resource="&gold;object"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/linguistics/gold/infixedIn -->

    <owl:ObjectProperty rdf:about="&gold;infixedIn">
