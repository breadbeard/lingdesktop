        <rdfs:subPropertyOf rdf:resource="&gold;dataStructureRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/linguistics/gold/hasType -->

    <owl:ObjectProperty rdf:about="&gold;hasType">
        <rdfs:label xml:lang="en"></rdfs:label>
